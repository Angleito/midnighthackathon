# Development environment configuration for ZK Ocean Combat

# Enable prover server for real testnet deployment
VITE_DISABLE_PROVER=false

# Enable WebSocket connections for testnet
VITE_DISABLE_WEBSOCKET=false

# Prover server URI (Docker container)
VITE_PROVER_SERVER_URI=http://localhost:6300

# Network configuration (defaults to Midnight testnet)
# VITE_INDEXER_URI=https://indexer.testnet.midnight.network/api/v1/graphql
# VITE_SUBSTRATE_NODE_URI=https://rpc.testnet.midnight.network

# Development mode settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=warn

# Wallet configuration for development
# Use 'programmatic' to bypass extension issues, 'extension' for extension only, 'auto' for both
VITE_WALLET_MODE=auto

# Force development mode if extension issues persist
VITE_FORCE_DEV_MODE=false

# Game configuration
VITE_GAME_DEBUG=false

# Connection timeouts for development (in milliseconds)
VITE_CONNECTION_TIMEOUT=10000
VITE_RETRY_ATTEMPTS=3